import "@cadl-lang/rest";

using Cadl.Http;

@doc("Error")
model Error {
  status: int32;
  message: string;
}

@doc("AutoRest Time Test Service")
@route("/time")
namespace Time {
  @doc("Get time value \"11:34:56\"")
  @get
  op get(): OkResponse<plainTime> | Error;

  @doc("Put time value \"08:07:56\"")
  @put
  op put(@body value: plainTime): OkResponse<{}> | Error;
}
